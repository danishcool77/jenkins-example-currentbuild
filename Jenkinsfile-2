pipeline {
  agent {
  label 'DANISH_MACHINE'}
  stages {
    stage('Hello') {
      steps {
        sh 'echo Hello World'
        echo 1 currentBuild.currentResult
      }
      post {
        success {
          script {
            currentBuild.result = 'FAILURE'
            echo 2 currentBuild.currentResult
          }
        }
      }
    }
  }
  post {
    always {
      echo 3 currentBuild.currentResult
       echo 4 currentBuild.result
    }
  }
}
